
{
    "version": 2,
    "builds": [
        {
        "src": "PDF_VIEWER/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb",
         "runtime": "python3.11.4" }
        },
        {
        "src":"build_files.sh",
        "use": "@vercel/static",
        "config": {
            "distDir":"staticfiles_build"
        }

        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "PDF_VIEWER/wsgi.py"
        }
    ]
}